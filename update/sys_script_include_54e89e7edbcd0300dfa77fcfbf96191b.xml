        <script><![CDATA[var Snippet_DAO = Class.create();
Snippet_DAO.prototype = {
    initialize: function() {
    },

	getCount: function(tableName, encodedQuery) {
		/******************
		* Get counts
		******************/
		
		var count;
		var ga = new GlideAggregate(tableName);
		ga.addAggregate('count');
		ga.query();
		while(ga.next()) {
			count = ga.getAggregate('count');
		}
		
		return count;
	},
	
    type: 'Snippet_DAO'
};]]></script>
